{
  "deno.enable": true,
  "deno.config": "deno.json",
  "deno.importMap": "deno.json",
  "deno.unstable": [
    "net",
    "http",
    "fs",
    "ffi",
  ],
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "deno.codeLens.test": true,
  "deno.internalDebug": true,
  "deno.internalInspect": true,
  "deno.maxTsServerMemory": 6000,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.detectIndentation": false,
  "editor.indentSize": 2,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown][typescript][javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[mdc]": {
    "editor.defaultFormatter": "pla"
  },
  "cSpell.useGitignore": true,
  "cSpell.ignorePaths": [
    "deno.{json,jsonc}",
    "deno.lock",
    "build/**",
    "dist/**",
    "test/**",
    "data/**"
  ],
  "cSpell.dictionaries": [
    "!typescript"
  ],
  "cSpell.words": [
    "deno",
    "typeof",
    "zackiles",
    "Iles",
    "denoland",
    "endgroup",
    "revparse",
    "cursorignoreindex",
    "cursorignore",
    "zacharyiles",
    "asynciterable",
    "webgpu",
    "onclose",
    "Zipkin",
    "parentbased",
    "tracestate",
    "traceparent",
    "MILLIS",
    "msvc",
    "dotenv",
    "swapfile",
    "hiberfil",
    "dconf",
    "keyrings",
    "dbus",
    "dockerenv",
    "fseventsd",
    "modelcontextprotocol",
    "rwxr",
    "pids",
    "USERPROFILE",
    "unvalidated",
    "somevalue",
    "testuser",
    "cursorindexignore",
    "cursorrules",
    "mdast",
    "frontmatter",
    "SIGWINCH",
    "SIGINT",
    "SIGTERM",
    "SIGKILL",
    "SIGSTOP",
    "SIGCONT",
    "SIGQUIT",
    "SIGILL",
    "SIGTRAP",
    "SIGABRT",
    "SIGBUS",
    "SIGFPE",
    "SIGSEGV"
  ],
  "workbench.editorAssociations": {
    "*.mdc": "default"
  }
}
